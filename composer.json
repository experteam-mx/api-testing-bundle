{
    "name": "experteam/api-testing-bundle",
    "description": "Testing functionality bundle for APIs",
    "homepage": "https://github.com/experteam-mx/api-testing-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Experteam",
            "email": "info@experteam.com.ec"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-redis": "*",
        "snc/redis-bundle": "^4.6",
        "symfony/config": "6.4.*",
        "symfony/dependency-injection": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/http-kernel": "6.4.*",
        "symfony/test-pack": "^1.1"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Experteam\\ApiTestingBundle\\": "src/"
        }
    }
}
